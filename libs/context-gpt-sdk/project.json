{
  "name": "context-gpt-sdk",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/context-gpt-sdk/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project context-gpt-sdk --web",
  "targets": {
    "codegen": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx  openapi-typescript ./apps/context-gpt-backend/context-gpt.openapi.json -o ./libs/context-gpt-sdk/api-types/schema.d.ts"
      },
      "dependsOn": [
        {
          "projects": "context-gpt-backend",
          "target": "generate-spec"
        }
      ]
    },
    "e2e": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "dependsOn": ["context-gpt-backend:build"]
    },
    "test": {
      "command": "echo 'No tests to run for context-gpt-backend-e2e'"
    }
  }
}
